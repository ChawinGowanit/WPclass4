<%= form_with(model: @user) do |form| %>
	<h1>Login</h1>
	<div class="field">
	    <%= form.label :email %>
	    <%= form.text_field :email %>
	  </div>
	  <div class="field">
	    <%= form.label :pass %>
	    <%= form.password_field :pass %>
	  </div>
	    <div class="actions">
	    <%= form.submit %>
	  </div>
<% end %>